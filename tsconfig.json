{
    "extends": "astro/tsconfigs/strict",
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@/*": ["src/*"],
            "@components/*": ["src/components/*"],
            "@layouts/*": ["src/layouts/*"],
            "@styles/*": ["src/styles/*"],
            "@images/*": ["src/images/*"],
            "@icons/*": ["src/icons/*"]
        }
    },
    "exclude": ["references", "src/components/old"]
}
